<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

    <changeSet id="create_table_destination" author="livanov">
        <createTable tableName="destination">
            <column name="id" type="bigint">
                <constraints primaryKey="true"/>
            </column>

            <column name="name" type="varchar(255)"/>
        </createTable>
    </changeSet>

    <changeSet id="destination_sequence_generator" author="livanov">
        <createSequence sequenceName="destination_sequence_generator"
                        startValue="1000"
                        incrementBy="1"/>
    </changeSet>
</databaseChangeLog>